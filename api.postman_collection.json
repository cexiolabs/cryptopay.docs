	"event": [
		{
			"listen": "prerequest",
			"script": {
				"id": "6a7ebe94-dbfc-4248-a032-a45d08cd8398",
				"type": "text/javascript",
				"exec": [
					"// Libs",
					"const crypto = require(\"crypto-js\");",
					"const pc = require(\"postman-collection\");",
					"",
					"// See https://developers.cryptopay.band/api/#signing-a-message",
					"function signature(secret, timestamp) {",
					"",
					"    const method = request.method; // GET, POST, etc.",
					"    const url = pc.Property.replaceSubstitutions(request.url, [pm.environment.values]); ",
					"    const path = new pc.Url(url).getPath();",
					"",
					"    let body = request.data;     // Request body (empty for method GET)",
					"    if (body === undefined || Object.keys(body).length === 0) {",
					"        body = null;",
					"    }",
					"",
					"    let what;",
					"    if (body !== null) {",
					"        what = timestamp + method + path + body;",
					"    }",
					"    else {",
					"        what = timestamp + method + path;",
					"    }",
					"",
					"    const secretBytes = crypto.enc.Base64.parse(secret);",
					"    const signatureBytes = crypto.HmacSHA256(what, secretBytes);",
					"    const signature = crypto.enc.Base64.stringify(signatureBytes);",
					"",
					"    return signature;",
					"}",
					"",
					"function readEnvValue(name) {",
					"    const value = pm.environment.get(name);",
					"    if (value === undefined) {",
					"        throw new Error(`Environment variable '${name}' is not defined. Please define one.`);",
					"    }",
					"    return value;",
					"}",
					"",
					"if (!pm.environment.has(\"apiBaseUrl\")) {",
					"    pm.environment.set(\"apiBaseUrl\", \"https://sandbox.cryptopay.band\");",
					"} else {",
					"    let apiUrl = pm.environment.get(\"apiBaseUrl\")",
					"    while(apiUrl.endsWith(\"/\")) {",
					"        apiUrl = apiUrl.substring(0, apiUrl.length - 1);",
					"    }",
					"    pm.environment.set(\"apiBaseUrl\", apiUrl);",
					"}",
					"",
					"const key = readEnvValue(\"apiKey\");",
					"const passphrase = readEnvValue(\"apiPassphrase\");",
					"const secret = readEnvValue(\"apiSecret\");",
					"",
					"const timestamp = Date.now() / 1000;",
					"const sign = signature(secret, timestamp);",
					"",
					"// Save variables to use inside requests",
					"pm.variables.set(\"CP-ACCESS-TIMESTAMP\", timestamp);",
					"pm.variables.set(\"CP-ACCESS-KEY\", key);",
					"pm.variables.set(\"CP-ACCESS-PASSPHRASE\", passphrase);",
					"pm.variables.set(\"CP-ACCESS-SIGN\", sign);",
					""
				]
			}
